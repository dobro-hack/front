\n        <tr>\n          <th>Название объявления</th>\n          <th>Статус</th>\n          <th>Трансляция</th>\n          <th>Бюджет</th>\n          <th>Потрачено</th>\n          <th>Результат</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr\n          v-for=\"(request, index) in requests\"\n          :key=\"index\"\n          @click=\"setSelectedRequest(request)\"\n        >\n          <td>{{ request.title }}</td>\n          <td>{{ request.status }}</td>\n          <td>{{ request.translation }}</td>\n          <td>{{ request.budget }}</td>\n          <td>{{ request.spent }}</td>\n          <td>{{ request.result }}</td>\n        </tr>\n      </tbody>\n    </table>\n    <div class=\"details\" v-if=\"selectedRequest\">\n      <h3>О заявке</h3>\n      <p>Парк: {{ selectedRequest.park }}</p>\n      <p>Маршрут: {{ selectedRequest.route }}</p>\n      <p>Тип: {{ selectedRequest.type }}</p>\n      <p>Планируемая дата: {{ selectedRequest.plannedDate }}</p>\n      <p>Вместимость: {{ selectedRequest.capacity }}</p>\n      <h4>Посетитель №1</h4>\n      <p>Фамилия: {{ selectedRequest.visitor1.lastName }}</p>\n      <p>Имя: {{ selectedRequest.visitor1.firstName }}</p>\n      <p>Отчество: {{ selectedRequest.visitor1.middleName }}</p>\n      <h4>Посетитель №2</h4>\n      <p>Фамилия: {{ selectedRequest.visitor2.lastName }}</p>\n      <p>Имя: {{ selectedRequest.visitor2.firstName }}</p>\n      <p>Отчество: {{ selectedRequest.visitor2.middleName }}</p>\n      <button @click=\"approve\">Согласовать</button>\n      <button @click=\"reject\">Отказать</button>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref } from \"vue\";\n\nconst requests = ref([\n  {\n    id: \"123\",\n    title: \"bozon_msk_07_2022\",\n    status: \"Одобрено\",\n    translation: \"Title\",\n    budget: 10000,\n    spent: 12432,\n    result: 124,\n    park: \"Название парка\",\n    route: \"Длинное Название маршрута\",\n    type: \"Групповая\",\n    plannedDate: \"27.06.2024\",\n    